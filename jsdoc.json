{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["./WebContent"],
        "includePattern": ".js$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {            
        "destination": "./docs",          
        "recurse": true,
        "tutorials": "./docs/tutorials", 
        "readme": "./readme.md"             
    }
}